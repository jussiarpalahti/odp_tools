{
  "compilerOptions": {
    "module": "system",
    "target":"ES6",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "init.js",
    "sourceMap": true
  },
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}